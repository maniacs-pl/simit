extern func matrix_neg<T>(A : matrix[T,T](float)) -> (B : matrix[T,T](float));

element Vertex
  a : float;
  b : float;
end

element Edge
  e : float;
end

extern V : set{Vertex};
extern E : set{Edge}(V,V);

func f(e : Edge, v : (Vertex*2)) -> (A : tensor[V,V](float))
  A(v(0),v(0)) = e.e;
  A(v(0),v(1)) = e.e;
  A(v(1),v(0)) = e.e;
  A(v(1),v(1)) = e.e;
end

export func main()
  B = map f to E reduce +;
  A = matrix_neg(B);
  V.a = A * V.b;
end
